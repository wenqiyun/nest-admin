    "ioredis": "^5.2.4",
    "js-yaml": "^4.1.0",
    "log4js": "^6.7.1",
    "mime-types": "^2.1.35",
    "mysql2": "^2.3.3",
    "node-xlsx": "^0.21.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "path-to-regexp": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "shortid": "^2.2.16",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.6.0",
    "ts-enum-util": "^4.0.2",
